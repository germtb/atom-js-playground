{
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "arrowFunctions": true,
      "blockBindings": true,
      "classes": true,
      "defaultParams": true,
      "destructuring": true,
      "modules": true,
      "objectLiteralComputedProperties": true,
      "templateStrings": true,
      "jsx": true
    }
  },
  "rules": {
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-empty": 2,
    "no-func-assign": 2,
    "no-inner-declarations": 2,
    "no-unreachable": 2,
    "no-unexpected-multiline": 0,
    "no-sequences": 2,
    "consistent-return": 0,
    "curly": [2, "all"],
    "no-else-return": 2,
    "no-multi-spaces": 0,
    "no-console": 0,
    "strict": 0,
    "no-var": 2,
    "no-shadow": 0,
    "no-unused-vars": [2, {"vars": "all", "args": "none"}],
    "no-undef": 2,
    "no-use-before-define": 0,
    "prefer-const": 2,
    "no-const-assign": 2,
    "no-new-object": 2,
    "no-useless-concat": 2,
    "no-implicit-coercion": 2,
    "no-this-before-super": 2,
    "object-shorthand": 2,
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/react-in-jsx-scope": 2,
    "mocha/no-exclusive-tests": "error",
    "mocha/no-skipped-tests": "error",
    "mocha/no-pending-tests": "error"
  },
  "env": {
    "node": true,
    "browser": true,
    "mocha": true,
    "es6": true
  },
  "globals": {
    "atom": true,
  },
  "plugins": [
    "react",
    "mocha"
  ]
}
